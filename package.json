{
  "name": "devdenbot",
  "version": "7.8.3",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "version": "echo $npm_package_version",
    "lint": "npx eslint 'src/**/*.ts' --fix",
    "build": "tsc --build --clean tsconfig.json && tsc --build tsconfig.json",
    "build-prod": "tsc -p tsconfig.production.json && ([ -f bin/Config.prod.js ] && mv -f bin/Config.prod.js bin/Config.js) || true && yarn sentry:sourcemaps",
    "watch-dev": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "start-built": "node --import bin/instrument.mjs --trace-warnings bin/index.js",
    "build-and-run": "yarn build && yarn start-built",
    "build-and-run-prod": "yarn lint && yarn build-prod && yarn start-built",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org developer-den-59 --project discord-bot ./bin && sentry-cli sourcemaps upload --org developer-den-59 --project discord-bot ./bin"
  },
  "keywords": [],
  "author": "Developer Den",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "@discordjs/rest": "^2.4.0",
    "@microsoft/eslint-formatter-sarif": "3.1.0",
    "@sentry/cli": "^2.38.0",
    "@sentry/integrations": "^7.119.2",
    "@sentry/node": "^8.35.0",
    "@sentry/profiling-node": "^8.35.0",
    "@sentry/tracing": "^7.114.0",
    "@sequelize/core": "^7.0.0-alpha.43",
    "@sequelize/sqlite3": "^7.0.0-alpha.43",
    "bufferutil": "^4.0.8",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.37.103",
    "discord.js": "^14.16.3",
    "djs-slash-helper": "https://github.com/bristermitten/djs-slash-helper.git#commit=1302255f21e14aa5c7bb987c11c7be53b1058f65",
    "dotenv": "^16.4.5",
    "erlpack": "^0.1.4",
    "expiry-map": "^2.0.0",
    "lru-cache": "^11.0.1",
    "mariadb": "^3.4.0",
    "node-canvas": "^2.9.0",
    "node-fetch": "^3.3.2",
    "node-gyp": "^10.2.0",
    "node-schedule": "^2.1.1",
    "opentype.js": "^1.3.4",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pg-types": "^4.0.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "string-similarity": "^4.0.4",
    "tslib": "^2.8.0",
    "utf-8-validate": "^6.0.4",
    "winston": "^3.15.0",
    "yaml": "^2.6.0",
    "zlib-sync": "^0.1.9"
  },
  "repository": "https://github.com/TheDeveloperDen/DevDenBot.git",
  "devDependencies": {
    "@sentry/types": "^8.35.0",
    "@types/node": "^22.8.0",
    "@types/node-schedule": "^2.1.7",
    "@types/opentype.js": "^1.3.8",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "@types/sqlite3": "^3.1.11",
    "@types/string-similarity": "^4.0.2",
    "@types/validator": "^13.12.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "ts-standard": "^12.0.2",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@3.5.1"
}
