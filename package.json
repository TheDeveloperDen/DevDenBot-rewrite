{
  "name": "devdenbot",
  "version": "7.0.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "version": "echo $npm_package_version",
    "lint": "npx eslint 'src/**/*.ts' --fix",
    "build": "tsc --build --clean tsconfig.json && tsc --build tsconfig.json",
    "build-prod": "tsc -p tsconfig.production.json && ([ -f bin/Config.prod.js ] && mv -f bin/Config.prod.js bin/Config.js) || true",
    "watch-dev": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "start-built": "node --trace-warnings bin/index.js",
    "build-and-run": "yarn build && yarn start-built",
    "build-and-run-prod": "yarn lint && yarn build-prod && yarn start-built"
  },
  "keywords": [],
  "author": "Developer Den",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.6.3",
    "@discordjs/rest": "^1.7.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@sentry/integrations": "^7.59.2",
    "@sentry/node": "^7.59.2",
    "@sentry/profiling-node": "^1.1.2",
    "@sentry/tracing": "^7.59.2",
    "@sequelize/core": "^7.0.0-alpha.27",
    "bufferutil": "^4.0.7",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.37.49",
    "discord.js": "^14.11.0",
    "djs-slash-helper": "^2.0.0",
    "dotenv": "^16.3.1",
    "erlpack": "^0.1.4",
    "expiry-map": "^2.0.0",
    "mariadb": "^3.2.0",
    "node-canvas": "^2.9.0",
    "node-fetch": "^3.3.1",
    "node-gyp": "^9.4.0",
    "node-schedule": "^2.1.1",
    "opentype.js": "^1.3.4",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "pg-types": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "string-similarity": "^4.0.4",
    "tslib": "^2.6.0",
    "utf-8-validate": "^6.0.3",
    "winston": "^3.10.0",
    "yaml": "^2.3.1",
    "zlib-sync": "^0.1.8"
  },
  "repository": "https://github.com/TheDeveloperDen/DevDenBot.git",
  "devDependencies": {
    "@sentry/types": "^7.59.2",
    "@types/node": "^20.4.2",
    "@types/node-schedule": "^2.1.0",
    "@types/opentype.js": "^1.3.4",
    "@types/pg": "^8.10.2",
    "@types/sequelize": "^4.28.15",
    "@types/sqlite3": "^3.1.8",
    "@types/string-similarity": "^4.0.0",
    "@types/validator": "^13.7.17",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "ts-standard": "^12.0.2",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.5.1"
}
